{
	"name": "Convert_Int",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "STAGE_SCD2_SP",
						"type": "DatasetReference"
					},
					"name": "ConvertInt"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          empid as integer,",
				"          empname as string,",
				"          gender as string,",
				"          country as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> ConvertInt",
				"ConvertInt derive(empid = toInteger(empid),",
				"          empname = toString(empname),",
				"          gender = ERROR_FUNCTION(''),",
				"          country = ERROR_FUNCTION('')) ~> derivedColumn1"
			]
		}
	}
}